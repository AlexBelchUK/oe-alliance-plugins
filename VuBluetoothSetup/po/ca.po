msgid ""
"Connection failed.\n"
"Another audio device is connected. (%s)"
msgstr ""
"Conexió fallida.\n"
"S'ha connectat un altre dispositiu d'àudio. (%s)"

#, python-format
msgid "Current FW version : %d"
msgstr "Versió de firmware actual : %d"

#, python-format
msgid ""
"Current FW version : %d\n"
"New FW version : %d"
msgstr ""
"Versió actual de firmware : %d\n"
"Nova versió de firmware : %d"

msgid "Device is disabling..."
msgstr "Desactivant el dispositiu..."

msgid "Device is enabled."
msgstr "Dispositiu activat."

msgid "Device is enabling..."
msgstr "Activant el dispositiu..."

msgid "Disable"
msgstr "Desactivar"

msgid "Disconnect"
msgstr "Desconnectar"

#, python-format
msgid "Disconnect %s failed!!"
msgstr "Desconnexió de %s fallida!!"

